}
.record-outside {
    width: 100vw;
    height: 100vh;
    background-color: #fff;
}
.tab-bar {
    width: 750rpx;
    height: 70rpx;
    border-bottom: 1px solid #eee;
}
.tab-bar text {
    display: inline-block;
    width: 33%;
    line-height: 70rpx;
    float: left;
    text-align: center;
    font-size: 28rpx;
}
.tab-bar text.thisover {
    color: #0099e9;
}
.record-tit {
    display: block;
    width: 700rpx;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1px solid #eee;
    margin: 0 25rpx;
    text-align: left;
    font-size: 30rpx;
}
.record-liebiao-tit {
    width: 700rpx;
    height: 60rpx;
    background-color: #eee;
    margin: 20rpx 25rpx 0;
    border: 1px solid #eee;
    text-align: center;
}
.record-liebiao-tit text {
    display: inline-block;
    line-height: 60rpx;
    width: 20%;
    float: left;
    font-size: 24rpx;
    color: #666;
}
.record-liebiao-tit text.time-long {
    width: 40%;
}
.record-liebiao-ul {
    display: block;
    width: 700rpx;
    margin: 0 25rpx 50rpx;
    overflow: hidden;
    border: 1px solid #eee;
}
.record-liebiao-ul ._li {
    display: block;
    overflow: hidden;
    height: 50rpx;
    text-align: center;
}
.record-liebiao-ul ._li.thisover {
    background-color: #f9f9f9;
}
.record-liebiao-ul ._li text {
    display: inline-block;
    width: 20%;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 24rpx;
    color: #666;
    float: left;
}
.record-liebiao-ul ._li text.time-long {
    width: 40%;
}
