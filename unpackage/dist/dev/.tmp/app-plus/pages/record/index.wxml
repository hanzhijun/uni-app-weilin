<view class="record-outside" bind:__l="__l"><view class="tab-bar"><text data-event-opts="{{[['tap',[['changeNav',['1']]]]]}}" class="{{[tabNum=='1'?'thisover':'']}}" bindtap="__e">今日</text><text data-event-opts="{{[['tap',[['changeNav',['2']]]]]}}" class="{{[tabNum=='2'?'thisover':'']}}" bindtap="__e">近三日</text><text data-event-opts="{{[['tap',[['changeNav',['3']]]]]}}" class="{{[tabNum=='3'?'thisover':'']}}" bindtap="__e">近一周</text></view><block wx:if="{{historyData!=''}}"><view class="qiun-columns"><view class="qiun-bg-white qiun-title-bar qiun-common-mt"><view class="qiun-title-dot-light">折线图（横屏图表）</view></view><block wx:if="{{historyData!=''}}"><view class="qiun-charts-rotate"><canvas class="charts-rotate" canvas-id="canvasLineB" id="canvasLineB" data-event-opts="{{[['touchstart',[['touchLineB',['$event']]]]]}}" bindtouchstart="__e"></canvas></view></block><view class="qiun-bg-white qiun-title-bar qiun-common-mt"><view class="qiun-title-dot-light">区域图</view></view><view class="qiun-charts"><canvas class="charts" canvas-id="canvasArea" id="canvasArea" data-event-opts="{{[['touchstart',[['touchArea',['$event']]]]]}}" bindtouchstart="__e"></canvas></view><view class="qiun-bg-white qiun-title-bar qiun-common-mt"><view class="qiun-title-dot-light">表格数据</view></view><view class="record-liebiao-tit"><text class="time-long">时间</text><text>心率/分</text><text>呼吸率/分</text><text>体动值</text></view><block wx:if="{{historyData}}"><view class="record-liebiao-ul _ul"><block wx:for="{{historyData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['_li',index%2==1?'thisover':'']}}"><text class="time-long">{{item.time}}</text><text>{{item.heart?item.heart:'--'}}</text><text>{{item.breath?item.breath:'--'}}</text><text>{{item.motion?item.motion:'--'}}</text></view></block></view></block></view></block><block wx:if="{{historyData==''&&firstLoad==1}}"><view class="qiun-columns"><view class="_ul"><view style="padding:180rpx 0;overflow:hidden;display:block;font-size:36rpx;text-align:center;color:#999;" class="_li">此时间段没有有效数据!</view></view></view></block><block wx:if="{{warnNing=='1'}}"><view class="base-box warn-box" catchtouchmove="true"><view class="gray-back"></view><view class="box-content"><text class="txt">{{warningText}}</text><view class="img"><image src="/static/warning.png"></image></view><view><button data-event-opts="{{[['tap',[['audioPause',['$event']]]]]}}" bindtap="__e">已知晓!</button></view></view></view></block><block wx:if="{{loading==1}}"><view class="base-box loading-box" catchtouchmove="true"><view class="gray-back"></view><view class="box-content"><view class="img"><image src="/static/loading.gif"></image></view><view class="txt">加载中...</view></view></view></block><block wx:if="{{showToast==1}}"><view class="show-toast"><text>{{toastTxt}}</text></view></block></view>