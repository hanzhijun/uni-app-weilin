
.login-line {
    overflow: hidden;
    padding: 20rpx 0;
}
.login-line-first {
    padding-top: 100rpx;
}
.section-title {
    display: inline-block;
    width: 200rpx;
    line-height: 68rpx;
    float: left;
    font-size: 14px;
    text-align: right;
}
.section-input {
    display: inline-block;
    width: 400rpx;
    height: 66rpx;
    line-height: 66rpx;
    border: 1px solid #0099e9;
    border-radius: 3px;
    float: left;
    font-size: 14px;
    padding-left: 20rpx;
}
.section-center {
    text-align: center;
    padding-top: 50rpx;
}
.section-post {
    display: inline-block;
    width: 500rpx;
    font-size: 16px;
    background-color: #0099e9;
    color: #fff;
    margin: 0 auto;
}
.section-post:active {
    background-color: #0082c5;
}

